@model CvProjectMVC.Models.Entity.TblYetenek
@{
    ViewBag.Title = "YeniYetenek";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Yetenek Ekle</h4>
<br />
<form method="post">
  
    @Html.Label("Yetenek Açıklama")
    @Html.TextBoxFor(x => x.Yetenek, new { @class = "form-control" })
    <br />
    @Html.Label("Derece")
    @Html.TextBoxFor(x => x.Oran, new { @class = "form-control" })
    <br />
    <button class="btn btn-primary">Kaydet</button>
</form>

