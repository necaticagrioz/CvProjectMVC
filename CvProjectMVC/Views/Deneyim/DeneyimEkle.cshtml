
@{
    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4>Deneyim Ekle</h4>
<br />
<form method="post">

    <label>Başlık</label>
    <input type="text" name="Baslik" class="form-control" placeholder="Başlık ekleyin" />
    <br />
    <input type="text" name="AltBaslik" class="form-control" placeholder="Alt başlık ekleyin" />
    <br />
    <input type="text" name="Tarih" class="form-control" placeholder="Tarih ekleyin" />
    <br />
    <textarea placeholder="Açıklama yazın" name="Aciklama" class="form-control" rows="5"></textarea>
    <br />
    <button class="btn btn-outline-success">Kaydet</button>
</form>
